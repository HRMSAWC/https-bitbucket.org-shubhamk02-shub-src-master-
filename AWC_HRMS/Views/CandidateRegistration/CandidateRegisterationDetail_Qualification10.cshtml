@model AWC_HRMS.Models.CandidateEducation


@{
    ViewData["Title"] = "CandidateRegisterationDetail";
     ViewData["btn"] = "Save";
     Layout = "~/Views/Shared/_LayoutCandidateForm.cshtml";
}


@*Respose.write "Bretval0: " + @ViewBag.myst*@

<div class="row">
    <div class="col-md-1 col-sm-12 Font_Color2">
    </div>

    <div class="col-md-8 col-sm-12 table-borderless" style="background-color:#08708d;font-family:Gill Sans, sans-serif; color:black">

        <center>High School Details</center>
        
        <form asp-action="CandidateRegisterationDetail_Qualification10" method="post" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger Font_Color"></div>
     

            <input asp-for="CandidateId" type="hidden" value="@ViewBag.myst"/>

            @*<div class="form-group">
                 <div class="space"> </div>
                <label asp-for="_10Course" class="control-label">Highest Qualification</label>
                <input asp-for="_10Course" class="form-control" required />
                <span asp-validation-for="_10Course" class="text-danger"></span>
            </div>*@
           
           
            <div class="form-group">
                 <div class="space"> </div>
                <label asp-for="_10SchoolName" class="control-label">School Name</label>
                <input asp-for="_10SchoolName" id="_10SchoolName" class="form-control" required />
                <span asp-validation-for="_10SchoolName" class="text-danger"></span>
            </div>

            <div class="form-group">
                 <div class="space"> </div>
                <label asp-for="_10BoardName" class="control-label">Board University</label>
                <input asp-for="_10BoardName" id="_10BoardName" class="form-control" required />
                <span asp-validation-for="_10BoardName" class="text-danger"></span>
            </div>

             <div class="form-group">
                 <div class="space"> </div>
                <label asp-for="_10Stream" class="control-label">Stream</label>
                <input asp-for="_10Stream" id="_10Stream" class="form-control" required />
                <span asp-validation-for="_10Stream" class="text-danger"></span>
            </div>


             <div class="form-group">
                  <div class="space"> </div>
                <label asp-for="_10YearOfPassing" class="control-label">Year of Passing</label>
                <input asp-for="_10YearOfPassing" id="_10YearOfPassing" required class="form-control" />
                <span asp-validation-for="_10YearOfPassing" class="text-danger"></span>
            </div>
            
            <div class="form-group">
                 <div class="space"> </div>
                <label asp-for="_10PercentageCgpa" class="control-label">GCP/Percentage</label>
                <input asp-for="_10PercentageCgpa" id="_10PercentageCgpa" required class="form-control" />
                <span asp-validation-for="_10PercentageCgpa" class="text-danger"></span>
            </div>

           @*  <div class="form-group">
                  <div class="space"> </div>
                <label asp-for="_10MarkSheet" class="control-label">Marksheet</label>
                <input type="file" asp-for="_10MarkSheet" required class="form-control" />
                <span asp-validation-for="_10MarkSheet" class="text-danger"></span>
            </div>
            
           
            <div class="form-group">
                 <div class="space"> </div>
                <label asp-for="_10DegreePassingCertificate" class="control-label">Degree</label>
                <input type="file" name="file" required asp-for="_10DegreePassingCertificate" class="form-control" />
                <span asp-validation-for="_10DegreePassingCertificate" class="text-danger"></span>
            </div>*@
           
           
            <div class="form-group">
                <div class="space"> </div>
                
                @Html.ActionLink("Back", "CandidateRegisterationDetail_Qualification12", new { id=ViewBag.myst },new{@class = "btn btn-info"})
                <input type="submit" button class="btn btn-info" value="Save" formaction="" />
                <!-- <input type="button" value="Preview" onclick="Response.redirect('CandidateRegisterationDetailNext')" class="btn button_color1" />-->
                @Html.ActionLink("Next", "CandidateRegisterationEmployment", new { id=ViewBag.myst },new{@class = "btn btn-info"})
                
            </div>
             </br>
        </form>
        @if (@ViewData["reason"] == null)
        {

        }
        else
        {
            <div class="alert alert-success text-center">@ViewData["reason"]</div>
        }
    </div>
</div>

<script type="text/javascript">

    $(window).on('load', function () {
        //alert('All assets are loaded');
        //debugger;
        var data = "@ViewBag.Vals2";
        if (data != "") {

            var sptval0 = data.split("|");


            document.getElementById("_10SchoolName").value = sptval0[0];
            document.getElementById("_10BoardName").value = sptval0[1];
            document.getElementById("_10Stream").value = sptval0[2];
            document.getElementById("_10YearOfPassing").value = sptval0[3];
            document.getElementById("_10PercentageCgpa").value = sptval0[4];

        }

    });

</script>